VAR
    x0 : boolean;
    x1 : boolean;
    x2 : boolean;
    x3 : boolean;
    x4 : boolean;
    x5 : boolean;
    x6 : boolean;
    x7 : boolean;

INIT
        not x0;

INIT
        not x1;

INIT
        not x2;

INIT
        not x3;

INIT
        not x4;

INIT
        not x5;

INIT
        not x6;

INIT
        not x7;

TRANS
        next(x0) <-> not x0;

TRANS
        next(x1) <-> ( x0 ? not x1 : x1 );

TRANS
        next(x2) <-> ( x0 and x1 ? not x2 : x2 );

TRANS
        next(x3) <-> ( x0 and x1 and x2 ? not x3 : x3 );

TRANS
        next(x4) <-> ( x0 and x1 and x2 and x3 ? not x4 : x4 );

TRANS
        next(x5) <-> ( x0 and x1 and x2 and x3 and x4 ? not x5 : x5 );

TRANS
        next(x6) <-> ( x0 and x1 and x2 and x3 and x4 and x5 ? not x6 : x6 );

TRANS
        next(x7) <-> ( x0 and x1 and x2 and x3 and x4 and x5 and x6 ? x7 : x7 );


