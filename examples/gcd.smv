#word-width 64
MODULE main

@input
VAR a, b : uint;
VAR x, y : uint;

INIT
    x = a; -- 5175833502;

INIT
   y = b; -- 520244859288;

TRANS
   next(x) = y;

TRANS
   next(y) = x % y;

@hidden
DEFINE TARGET := y = 0;

-- to calculate gcd issue the command:
-- >> reach TARGET;
